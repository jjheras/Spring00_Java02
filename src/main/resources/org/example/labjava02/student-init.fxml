<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.example.labjava02.StudentController"
          alignment="CENTER" hgap="10" vgap="10">
    <padding>
        <Insets top="25" right="25" bottom="25" left="25"/>
    </padding>

    <Button fx:id="btnCrearArchivo" onAction="#btnCrearClick" text="Crear archivo" GridPane.columnIndex="0" GridPane.rowIndex="0" prefWidth="150"/>
    <Button fx:id="btnCargarArchivo" onAction="#btnCargarClick" text="Cargar archivo" GridPane.columnIndex="0" GridPane.rowIndex="1" prefWidth="150"/>

</GridPane>
